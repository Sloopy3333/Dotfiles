#!/bin/sh
wm=qtile
userresources=$HOME/.config/X11/Xresources
usermodmap=$HOME/.config/X11/Xmodmap
xrdb -merge $userresources
#xmodmap $usermodmap &
#stalonetray -c ~/.config/stalonetrayrc/.stalonetrayrc &
#pasystray &
#nm-applet &
#picom &
xbacklight -set 5 &
sudo $HOME/scripts/powertop_fix.sh &
